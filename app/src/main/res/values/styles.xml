<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

	<style name="Button.Start" parent="Widget.AppCompat.Button">
		<item name="colorButtonNormal">#e67e22</item>
		<item name="colorControlHighlight">#595959</item>
		<item name="android:textColor">#595959</item> </style>

	<style name="Button.Yes" parent="Widget.AppCompat.Button">
		<item name="android:padding">0dp</item>
		<item name="colorButtonNormal">#51894e</item>
		<item name="colorControlHighlight">#595959</item>
		<item name="android:textColor">#595959</item> </style>

	<style name="Button.No" parent="Widget.AppCompat.Button">
		<item name="android:padding">0dp</item>
		<item name="colorButtonNormal">#7dbbe6</item>
		<item name="colorControlHighlight">#595959</item>
		<item name="android:textColor">#595959</item> </style>

	<style name="Button.Next" parent="Widget.AppCompat.Button">
		<item name="android:padding">0dp</item>
		<item name="colorButtonNormal">#e6af7d</item>
		<item name="colorControlHighlight">#595959</item>
		<item name="android:textColor">#595959</item> </style>

</resources>
